{
    {% if not consul_join %}
    "bootstrap_expect": 3,
    {% endif %}
    {% if consul_join %}
    "start_join": ["{{ consul_join }}"],
    {% endif %}
    "server": true,
    "enable_syslog": true,
    "data_dir": "{{ consul_data_dir }}",
    "datacenter": "{{ ansible_ec2_placement_region }}",
    "recursor": "10.0.0.2"
}
